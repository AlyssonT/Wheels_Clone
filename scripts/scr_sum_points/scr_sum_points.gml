function sum_points(tile){
	blue_count += tile.blue_points;
	orange_count += tile.orange_points;
	hammer_count += tile.hammer_points;
	blue_tierup_count += tile.blue_tierup_points;
	orange_tierup_count += tile.orange_tierup_points;
}